{
    "name": "oro/commerce-infinitepay",
    "description": "OroCommerce InfinitePay Integration",
    "homepage": "https://github.com/orocommerce/OroInfinitePayBundle.git",
    "license": "OSL-3.0",
    "keywords": ["Oro", "InfinitePay", "Integration"],
    "authors": [
        {
            "name": "Oro, Inc",
            "homepage": "http://www.orocommerce.com"
        }
    ],
    "autoload": {
        "psr-4": {"Oro\\Bundle\\InfinitePayBundle\\": ""},
        "exclude-from-classmap": ["/Tests/"]
    },
    "require": {
        "php": ">=7.0",
        "oro/commerce": "1.5.*",
        "besimple/soap-client": "^0.2.6"
    },
    "repositories": {
        "oro": {
            "type": "composer",
            "url": "https://packagist.orocrm.com"
        },
        "asset-packagist": {
            "type": "composer",
            "url": "https://asset-packagist.orocrm.com"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "branch-alias": {
            "dev-master": "1.5.x-dev"
        }
    }
}
